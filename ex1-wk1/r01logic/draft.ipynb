   "source": [
    "from itertools import combinations\n",
    "def powerset(string):\n",
    "    res_ls = []\n",
    "    n = len(string)\n",
    "    for i in range(0,n+1):\n",
    "        for element in combinations(string,i):\n",
    "            res_ls.append(list(element))\n",
    "    return res_ls\n",
    "string=['x','y','z']\n",
    "powerset(string)"
   ]
